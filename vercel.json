{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api-docs",
        "dest": "src/main.ts"
      },
      {
        "src": "/",
        "status": 302,
        "headers": {
          "Location": "/api-docs"
        }
      }
    ]
  }
  